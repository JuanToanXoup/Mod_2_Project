<%= form_for @trail do |f|%>
    <br><%= f.label "Trail Photo" %>
    <%= f.file_field :trail_pic %></br>
    <br><%= f.label :name %>
    <%= f.text_field :name %></br>
    <br><%= f.label :location %>
    <%= f.text_field :location %></br>
    <br><%= f.label :length %>
    <%= f.text_field :length %></br>
    <br><%= f.label :duration %>
    <%= f.text_field :duration %></br>
    <br><%= f.label :difficulty %>
    <%= f.text_field :difficulty %></br>
    <br><%= f.label :type %>
    <%= f.text_field :trail_type %></br>
    
    <br><%= f.submit %>
<% end %>