<%= form_for @trail do |f|%>
    <%= f.label "Trail Photo" %>
    <%= f.file_field :trail_pic %><br>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :location %>
    <%= f.text_field :location %><br>
    <%= f.label :length %>
    <%= f.text_field :length %><br>
    <%= f.label :duration %>
    <%= f.text_field :duration %><br>
    <%= f.label :difficulty %>
    <%= f.text_field :difficulty %><br>
    <%= f.label :type %>
    <%= f.text_field :trail_type %><br>
    
    <%= f.submit %>
<% end %>