<%= form_for @user do |f|%>
    <%= f.label "Profile Photo" %>
    <%= f.file_field :avatar %><br>
    <%= f.label "Username" %>
    <%= f.text_field :username, class: "form-control" %><br>
    <%= f.label :name %>
    <%= f.text_field :name, class: "form-control" %><br>
    <%= f.label :age %>
    <%= f.text_field :age, class: "form-control" %><br>
    <%= f.label :skill_level %>
    <%= f.select :skill_level, [['Beginner','beginner'],['Intermediate','intermediate'],['Expert','expert']], class: "form-control"%><br>
    <%= f.label :address %><br>
    <%= f.text_area :address, size: "15x5", class: "form-control"%><br>
    <%= f.submit class: "btn btn-primary" %>
<% end %>